<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="Mannat Themes">
        <meta name="keyword" content="">

        <title>Scrum 3D Park</title>

        <!-- Theme icon -->
        <link rel="shortcut icon" href="/imagens/favicon.ico" type="image/x-icon" />

        <!-- Theme Css -->
        <link href="/assets/css/bootstrap.min.css" rel="stylesheet">
        <link href="/assets/css/slidebars.min.css" rel="stylesheet">
        <link href="/assets/css/icons.css" rel="stylesheet">
        <link href="/assets/css/menu.css" rel="stylesheet" type="text/css">
        <link href="/assets/css/style.css" rel="stylesheet">
        
    </head>

<style>

.box-chat {
  padding-top: 110px;
  height: 90vh;
}

.msger {
  max-width: 400px;
  min-width: 400px;
}

</style>

    <body class="sticky-header" bgcolor="#f0f0f0">
        
        <section class="bg-login">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-sm-12">
                        <div class="wrapper-page">
                            <div class="account-pages">
                                <div class="account-box">
                                    <div class="card m-b-30">
                                        <div class="card-body">
                                            <% if(typeof message !== 'undefined' && message !== ''){ %>
                                            <div class="alert alert-danger" role="alert">
                                                <%=message%>
                                            </div>
                                            <% } %>
                                             
                                            <form class="form mt-5 contact-form" action="/simulation" method="post">
                                                <div class="form-group ">
                                                    <div class="col-sm-12">
                                                        <input class="form-control form-control-line" name="email" type="text" placeholder="E-mail" required="required">
                                                    </div>
                                                </div>
                                                
                                                <div class="form-group">
                                                    <div class="col-sm-12 mt-4">
                                                        <button class="btn btn-primary btn-block" type="submit">Simulação Grátis</button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <!-- jQuery -->
        <script src="/assets/js/jquery-3.2.1.min.js"></script>
        <script src="/assets/js/popper.min.js"></script>
        <script src="/assets/js/bootstrap.min.js"></script>
        <script src="/assets/js/jquery-migrate.js"></script>
        <script src="/assets/js/modernizr.min.js"></script>
        <script src="/assets/js/jquery.slimscroll.min.js"></script>
        <script src="/assets/js/slidebars.min.js"></script>
        

        <!--app js-->
        <script src="/assets/js/jquery.app.js"></script>
    </body>
</html>